<h1>Homepage</h1>
<div class="post-page">
<h2 class="all-posts">All Posts</h2>
{{#each allPosts}}
<div class="post">
<section class="post-box" id="{{id}}">
    <div class="home-title">
        <h2>{{title}}</h2>
    </div>
    <div class="home-created-at">
        <p>posted on {{createdAt}}</p>
    </div>
</section>
</div>
    {{/each}}
</div>

   
    <script src="/js/home.js"></script>